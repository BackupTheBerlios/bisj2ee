<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSPY v2004 rel. 3 U (http://www.xmlspy.com) by kossi (NONE) --><xsl:stylesheet xmlns:ns="http://www.my-webbase.de/xml" version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">	<xsl:output method="xml" version="1.0" encoding="UTF-8" indent="yes"/>	<!-- ........................ KEYS ................................................................................................................................ -->	<xsl:key name="projectREF" match="ns:project" use="@id"/>	<xsl:key name="adviceREF" match="ns:advice" use="@id"/>	<xsl:key name="personREF" match="ns:person" use="ns:ID"/>	<!-- ........................ WORKGROUP ........................................................................................................................... -->	<xsl:template match="ns:workgroup">		<html>			<head>				<title>					<xsl:value-of select="name"/>				</title>				<style type="text/css">						body 		    {	background:       #ffffcc; font-family:     helvetica, arial, sans-serif ; text-align: justify}						a 			  		 { font-weight:      bold;    text-decoration: underline }						a:link        { color:            #000099; text-decoration: none }						a:visited     { color:            #111199; text-decoration: none }						a:hover       { background:       #eeeecc; color:           #000099 }						input:unknown {	background-color: #ffffcc }			</style>			</head>			<body>				<table>					<tr>						<td>							<img src="{ns:info/ns:logo}" alt="{ns:info/ns:logo/@alt}"/>							<br/>							<br/>						</td>						<td>							<big>								<big>									<a href="{ns:info/ns:institute/@href}inst/ag-nbi">										<xsl:value-of select="@name"/>									</a>									<br/>									<i>										<a href="{ns:info/ns:institute/@href}">											<xsl:value-of select="ns:info/ns:institute"/>										</a>, 								<a href="{ns:info/ns:department/@href}">											<xsl:value-of select="ns:info/ns:department"/>										</a>, 								<a href="{ns:info/ns:university/@href}">											<xsl:value-of select="ns:info/ns:university"/>										</a>									</i>								</big>							</big>						</td>					</tr>				</table>				<xsl:apply-templates/>			</body>		</html>	</xsl:template>	<!-- ........................ NEWS ................................................................................................................................ -->	<xsl:template match="ns:news">		<table cellspacing="0" cellpadding="0" border="0" width="100%">			<tr>				<td valign="top" width="20" bgcolor="#33cc00" height="20">					<br/>				</td>				<td valign="top" height="20">					<b>News<br/>					</b>				</td>			</tr>			<tr>				<td valign="top" width="20" height="20">					<br/>				</td>				<td valign="top" bgcolor="#99ff99">					<ul>						<xsl:for-each select="ns:notice">							<li>								<xsl:apply-templates select="."/>							</li>						</xsl:for-each>					</ul>				</td>			</tr>		</table>		<br/>	</xsl:template>	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->	<xsl:template match="ns:notice">		<xsl:value-of select="ns:date"/>:			<xsl:choose>			<xsl:when test="@type='Projektstart'">Start des Projekts: <a href="#{key('projectREF',@keyref)/@id}">					<xsl:value-of select="key('projectREF',@keyref)/ns:name"/>				</a>			</xsl:when>			<xsl:when test="@type='Update'">Hinweis im Netz: <a href="{key('adviceREF',@keyref)/@href}">					<xsl:value-of select="key('adviceREF',@keyref)"/>				</a>			</xsl:when>		</xsl:choose>	</xsl:template>	<!-- ........................ PEOPLE .............................................................................................................................. -->	<xsl:template match="ns:people">		<table cellspacing="0" cellpadding="0" border="0" width="100%">			<tbody>				<tr>					<td valign="top" width="20" bgcolor="#666666" height="20">						<br/>					</td>					<td valign="top" height="20">						<b>People</b>					</td>				</tr>				<tr>					<td valign="top" width="20" height="20">						<br/>					</td>					<td valign="top" bgcolor="#cccccc">						<table border="0" cellpadding="1" cellspacing="0">							<xsl:apply-templates/>						</table>						<a href="{@fomerMembersURL}">Former members of this group</a>					</td>				</tr>			</tbody>		</table>		<br/>	</xsl:template>	<xsl:template match="ns:person">		<tr>			<th align="left" colspan="2">				<a name="{ns:ID}"/>				<xsl:if test="@href">					<a href="{@href}">						<xsl:for-each select="ns:name/ns:title">							<xsl:value-of select="."/>							<xsl:text> </xsl:text>						</xsl:for-each>						<xsl:value-of select="ns:name/ns:first"/>						<xsl:text> </xsl:text>						<xsl:for-each select="ns:name/ns:middle">							<xsl:value-of select="."/>							<xsl:text> </xsl:text>						</xsl:for-each>						<xsl:value-of select="ns:name/ns:last"/>					</a>				</xsl:if>				<xsl:if test="not(@href)">					<xsl:for-each select="ns:name/ns:title">						<xsl:value-of select="."/>						<xsl:text> </xsl:text>					</xsl:for-each>					<xsl:value-of select="ns:name/ns:first"/>					<xsl:text> </xsl:text>					<xsl:for-each select="ns:name/ns:middle">						<xsl:value-of select="."/>						<xsl:text> </xsl:text>					</xsl:for-each>					<xsl:value-of select="ns:name/ns:last"/>				</xsl:if>			</th>		</tr>		<xsl:if test="ns:role">			<tr>				<td colspan="2">					<i>						<xsl:value-of select="ns:role"/>					</i>				</td>			</tr>		</xsl:if>		<xsl:if test="ns:directdial">			<tr>				<td>Durchwahl:</td>				<td>					<xsl:value-of select="ns:directdial"/>				</td>			</tr>		</xsl:if>		<xsl:if test="ns:email">			<tr>				<td>email:</td>				<td>					<a href="mailto:{ns:email}">						<xsl:value-of select="ns:email"/>					</a>				</td>			</tr>		</xsl:if>		<xsl:if test="ns:assigned-projects">			<tr>				<td valign="top">Projekte:</td>				<td>					<xsl:for-each select="ns:assigned-projects/ns:projectREF">						<a href="#{key('projectREF',.)/@id}">							<xsl:value-of select="key('projectREF',.)/ns:name"/>						</a>						<br/>					</xsl:for-each>				</td>			</tr>		</xsl:if>		<tr>			<th colspan="2" valign="middle" height="5px">				<hr/>			</th>		</tr>	</xsl:template>	<!-- ........................ TEACHING ............................................................................................................................ -->	<xsl:template match="ns:teaching">		<table cellspacing="0" cellpadding="0" border="0" width="100%">			<tbody>				<tr>					<td valign="top" width="20" bgcolor="#ff0000" height="20">						<br/>					</td>					<td valign="top" height="20">						<b>Lehre - Teaching</b>					</td>				</tr>				<tr>					<td valign="top" width="20" height="20">						<br/>					</td>					<td valign="top" bgcolor="#ffcccc">						<xsl:apply-templates/>					</td>				</tr>			</tbody>		</table>		<br/>	</xsl:template>	<xsl:template match="ns:courses">		<xsl:choose>			<xsl:when test="ns:course">				<xsl:apply-templates select="ns:term"/>:				<ul>					<xsl:apply-templates select="ns:course"/>				</ul>			</xsl:when>			<xsl:when test="ns:href">				<a href="{ns:href}">					<xsl:apply-templates select="ns:term"/>				</a>				<br/>			</xsl:when>		</xsl:choose>	</xsl:template>	<xsl:template match="ns:term">		<b>			<xsl:choose>				<xsl:when test="@season='WS'">Winter</xsl:when>				<xsl:when test="@season='SS'">Sommer</xsl:when>			</xsl:choose>semester 			<xsl:value-of select="@year"/>		</b>	</xsl:template>	<xsl:template match="ns:course">		<li>			<xsl:choose>				<xsl:when test="@courseType='lecture'">Vorlesung: </xsl:when>				<xsl:when test="@courseType='seminar'">Seminar: </xsl:when>				<xsl:when test="@courseType='practical'">Praktikum: </xsl:when>			</xsl:choose>			<xsl:if test="@href">				<a href="{@href}">					<xsl:value-of select="."/>				</a>			</xsl:if>			<xsl:if test="not(@href)">				<xsl:value-of select="."/>			</xsl:if>		</li>	</xsl:template>	<xsl:template match="ns:assignments">		<p>			<xsl:value-of select="ns:description"/>			<xsl:if test="@href"> (<a href="{@href}">Link</a>)</xsl:if>			<br/>			<xsl:apply-templates select="ns:advices"/>		</p>	</xsl:template>	<xsl:template match="ns:advices">			Bitte beachten Sie folgende Hinweise:			<ul>			<xsl:apply-templates select="ns:advice"/>		</ul>	</xsl:template>	<xsl:template match="ns:advice">		<li>			<xsl:if test="@href">				<a href="{@href}">					<xsl:value-of select="."/>				</a>			</xsl:if>			<xsl:if test="not(@href)">				<xsl:value-of select="."/>			</xsl:if>		</li>	</xsl:template>	<!-- ........................ RESEARCH ............................................................................................................................ -->	<xsl:template match="ns:research">		<table cellspacing="0" cellpadding="0" border="0" width="100%">			<tr>				<td valign="top" width="20" bgcolor="#33ccff" height="20">					<br/>				</td>				<td valign="top" height="20">					<b>Research<br/>					</b>				</td>			</tr>			<tr>				<td valign="top" width="20" height="20">					<br/>				</td>				<td valign="top" bgcolor="#ccffff">					<xsl:apply-templates/>				</td>			</tr>		</table>		<br/>	</xsl:template>	<xsl:template match="ns:tasks">		<xsl:if test="ns:task">Research of the group includes:					<ul>				<xsl:apply-templates/>			</ul>		</xsl:if>	</xsl:template>	<xsl:template match="ns:task">		<li>			<xsl:value-of select="."/>		</li>	</xsl:template>	<xsl:template match="ns:results">		<xsl:choose>			<xsl:when test="ns:result">Current research results are:			<ul>					<xsl:apply-templates/>				</ul>			</xsl:when>			<xsl:otherwise>There are no current results.</xsl:otherwise>		</xsl:choose>	</xsl:template>	<xsl:template match="ns:result">		<li>			<xsl:choose>				<xsl:when test="@href">					<a href="{@href}">						<xsl:choose>							<xsl:when test="@a">								<xsl:value-of select="@a"/>							</xsl:when>							<xsl:otherwise>								<xsl:value-of select="."/>							</xsl:otherwise>						</xsl:choose>					</a> - <xsl:text/>				</xsl:when>			</xsl:choose>			<xsl:value-of select="."/>		</li>	</xsl:template>	<xsl:template match="ns:projects">		<span style="text-decoration: underline">Funded Projects on <xsl:value-of select="@topic"/>		</span>		<br/>		<ul>			<xsl:apply-templates/>		</ul>	</xsl:template>	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->	<xsl:template match="ns:project">		<li>			<xsl:choose>				<xsl:when test="ns:description/ns:href">					<a href="{ns:description/ns:href}">						<xsl:value-of select="ns:name"/>					</a>				</xsl:when>				<xsl:otherwise>					<xsl:value-of select="ns:name"/>				</xsl:otherwise>			</xsl:choose>			<xsl:if test="ns:description/ns:english-version">				<a href="{ns:description/ns:english-version}">					<img height="12px" src="http://www.anbg.gov.au/images/flags/union-jack.gif" alt="Englische Version"/>				</a>			</xsl:if>			<br/>			<br/>			<xsl:value-of select="ns:brief-description"/>			<br/>			<br/>			<xsl:if test="ns:description/ns:abstract">				<xsl:value-of select="ns:description/ns:abstract"/>				<br/>				<br/>			</xsl:if>			<xsl:apply-templates select="ns:description/ns:workpackages"/>			<xsl:if test="ns:description/ns:contacts">				<h3>Kontake</h3>				<xsl:apply-templates select="ns:description/ns:contacts"/>				<br/>			</xsl:if>			<xsl:if test="ns:description/ns:initiators">				<h3>Projektinitiatoren</h3>				<xsl:apply-templates select="ns:description/ns:initiators"/>				<br/>			</xsl:if>			<xsl:if test="ns:description/ns:publications/ns:publication">				<xsl:apply-templates select="ns:description/ns:publications"/>				<br/>			</xsl:if>			<table cellspacing="0" border="0">				<tbody>					<xsl:if test="ns:partner">						<tr>							<th valign="top" align="right">Partner:</th>							<td valign="top">								<xsl:for-each select="ns:partner">									<xsl:choose>										<xsl:when test="@href">											<a href="{@href}">												<xsl:value-of select="."/>											</a>										</xsl:when>										<xsl:otherwise>											<xsl:value-of select="."/>										</xsl:otherwise>									</xsl:choose>									<br/>								</xsl:for-each>							</td>						</tr>						<tr>							<th>								<xsl:text> </xsl:text>							</th>						</tr>					</xsl:if>					<xsl:if test="ns:sponsor">						<tr>							<th valign="top" align="right">Sponsoren:</th>							<td valign="top">								<xsl:for-each select="ns:sponsor">									<xsl:choose>										<xsl:when test="@href">											<a href="{@href}">												<xsl:value-of select="."/>											</a>										</xsl:when>										<xsl:otherwise>											<xsl:value-of select="."/>										</xsl:otherwise>									</xsl:choose>									<br/>								</xsl:for-each>							</td>						</tr>					</xsl:if>				</tbody>			</table>			<br/>		</li>		<hr style="backgroud-color:white;color:darkcyan"/>	</xsl:template>	<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->	<xsl:template match="ns:workpackages">		<table border="0" width="100%">			<tbody>				<tr>					<th colspan="2" align="left">						<h3>Arbeitspakete</h3>					</th>				</tr>				<xsl:for-each select="ns:package">					<tr>						<th valign="top">							<xsl:value-of select="@id"/>						</th>						<td>							<xsl:choose>								<xsl:when test="@href">									<a href="{@href}">										<xsl:value-of select="."/>									</a>								</xsl:when>								<xsl:otherwise>									<xsl:value-of select="."/>								</xsl:otherwise>							</xsl:choose>						</td>					</tr>				</xsl:for-each>			</tbody>		</table>	</xsl:template>	<xsl:template match="ns:contacts">		<xsl:call-template name="personList"/>	</xsl:template>	<xsl:template match="ns:initiators">		<table border="0" width="100%">			<tr>				<xsl:for-each select="ns:person">					<td valign="top">						<xsl:call-template name="Initiator">							<xsl:with-param name="Initiator" select="."/>						</xsl:call-template>					</td>				</xsl:for-each>				<xsl:for-each select="ns:personREF">					<td valign="top">						<xsl:call-template name="Initiator">							<xsl:with-param name="Initiator" select="key('personREF',.)"/>						</xsl:call-template>					</td>				</xsl:for-each>			</tr>		</table>	</xsl:template>	<xsl:template name="Initiator">		<xsl:param name="Initiator"/>		<img src="{$Initiator/ns:logo}" height="100" alt="Logo"/>		<br/>		<br/>		<xsl:for-each select="$Initiator/ns:name/ns:title">			<xsl:value-of select="."/>		</xsl:for-each>		<xsl:text> </xsl:text>		<xsl:value-of select="$Initiator/ns:name/ns:first"/>		<xsl:text> </xsl:text>		<xsl:for-each select="$Initiator/ns:name/ns:middle">			<xsl:value-of select="."/>			<xsl:text> </xsl:text>		</xsl:for-each>		<xsl:value-of select="$Initiator/ns:name/ns:last"/>		<br/>		<xsl:value-of select="$Initiator/ns:role"/>		<br/>		<br/>		<xsl:apply-templates select="$Initiator/ns:address"/>		<br/>		Tel: <xsl:value-of select="$Initiator/ns:phone"/>		<br/>		Email: <a href="mailto:{$Initiator/ns:email}">			<xsl:value-of select="$Initiator/ns:email"/>		</a>	</xsl:template>	<xsl:template match="ns:publications">		<h3>Publikationen</h3>		<table border="0" cellpadding="6">			<tr>				<th valign="top" align="left">Autoren</th>				<th valign="top" align="left">Titel</th>			</tr>			<xsl:apply-templates select="ns:publication"/>		</table>	</xsl:template>	<xsl:template match="ns:publication">		<tr>			<td>				<xsl:for-each select="ns:authors/ns:personREF">					<xsl:apply-templates select="."/>					<br/>				</xsl:for-each>			</td>			<td valign="top">				<xsl:choose>					<xsl:when test="@href">						<a href="{@href}">							<xsl:value-of select="ns:title"/>						</a>					</xsl:when>					<xsl:otherwise>						<xsl:value-of select="ns:title"/>					</xsl:otherwise>				</xsl:choose>			</td>		</tr>	</xsl:template>	<!-- ............................................................................................... INFO .................................................................................................. -->	<xsl:template match="ns:info">		<table cellspacing="0" cellpadding="0" border="0" width="100%">			<tbody>				<tr>					<td valign="top" width="20" bgcolor="#ff9900" height="20">						<br/>					</td>					<td valign="top" height="20">						<b>How to reach us<br/>						</b>					</td>				</tr>				<tr>					<td valign="top" width="20" height="20">						<br/>					</td>					<td valign="top" align="left" bgcolor="FFCC66">						<xsl:if test="ns:logo">							<img src="{ns:logo}" alt="Logo"/>						</xsl:if>						<xsl:if test="not(ns:university/@href)">							<xsl:value-of select="ns:university"/>						</xsl:if>						<xsl:if test="ns:university/@href">							<a href="{ns:university/@href}">								<xsl:value-of select="ns:university"/>							</a>						</xsl:if>						<br/>						<xsl:if test="not(ns:department/@href)">							<xsl:value-of select="ns:department"/>						</xsl:if>						<xsl:if test="ns:department/@href">							<a href="{ns:department/@href}">								<xsl:value-of select="ns:department"/>							</a>						</xsl:if>						<br/>						<xsl:if test="not(ns:institute/@href)">							<xsl:value-of select="ns:institute"/>						</xsl:if>						<xsl:if test="ns:institute/@href">							<a href="{ns:institute/@href}">								<xsl:value-of select="ns:institute"/>							</a>						</xsl:if>						<br/>						<hr style="color:darkorange"/>						<h3>Adresse</h3>						<xsl:value-of select="ns:address/ns:name"/>						<br/>						<xsl:value-of select="ns:address/ns:street"/>						<br/>						<xsl:value-of select="ns:address/ns:zipcode"/>						<br/>						<xsl:value-of select="ns:address/ns:city"/>						<br/>						<xsl:value-of select="ns:address/ns:country"/>						<br/>						<hr style="color:darkorange"/>						<h3>Telefon</h3>Vorwahl: <xsl:value-of select="ns:telephone/ns:prefix"/>						<br/>						Standarddurchwahl: <xsl:value-of select="ns:telephone/ns:default-directdial"/>						<br/>						<xsl:if test="ns:telephone/ns:fax-directdial">Faxdurchwahl: <xsl:value-of select="ns:telephone/ns:fax-directdial"/>						</xsl:if>						<br/>						<hr style="color:darkorange"/>						<h3>Office</h3>						<xsl:apply-templates select="ns:public-office/ns:address"/>						<br/>						<b>Ã–ffnungszeiten</b>						<br/><br/>						<xsl:value-of select="ns:public-office/ns:open"/>					</td>				</tr>			</tbody>		</table>		<br/>	</xsl:template>	<!-- .............................................................................................................................................................. -->	<xsl:template match="ns:personREF">		<a href="#{.}">			<xsl:for-each select="key('personREF',.)/ns:name/ns:title">				<xsl:value-of select="."/>				<xsl:text> </xsl:text>			</xsl:for-each>			<xsl:value-of select="key('personREF',.)/ns:name/ns:first"/>			<xsl:text> </xsl:text>			<xsl:for-each select="key('personREF',.)/ns:name/ns:middle">				<xsl:value-of select="."/>				<xsl:text> </xsl:text>			</xsl:for-each>			<xsl:text> </xsl:text>			<xsl:value-of select="key('personREF',.)/ns:name/ns:last"/>		</a>	</xsl:template>	<xsl:template name="personList">		<xsl:for-each select="ns:personREF">			<xsl:apply-templates select="."/>		</xsl:for-each>	</xsl:template>	<xsl:template match="ns:personREF">		<a href="#{.}">			<xsl:apply-templates select="key('personREF',.)/ns:name"/>		</a>	</xsl:template>	<xsl:template match="ns:name">		<xsl:for-each select="ns:title">			<xsl:value-of select="."/>			<xsl:text> </xsl:text>		</xsl:for-each>		<xsl:value-of select="ns:first"/>		<xsl:text> </xsl:text>		<xsl:for-each select="ns:middle">			<xsl:value-of select="."/>			<xsl:text> </xsl:text>		</xsl:for-each>		<xsl:value-of select="ns:last"/>	</xsl:template>	<xsl:template match="ns:address">		<xsl:value-of select="ns:name"/>		<br/>		<xsl:value-of select="ns:street"/>		<br/>		<xsl:value-of select="ns:zipcode"/>		<br/>		<xsl:value-of select="ns:city"/>		<br/>		<xsl:value-of select="ns:country"/>		<br/>	</xsl:template>	<xsl:template match="*"/></xsl:stylesheet>